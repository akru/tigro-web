$(function() {
    $("#appendRobotWidget").dialog({
        autoOpen: false,
        show: { duration: 500 },
        hide: { duration: 500 },
        buttons: {
            Ok: function(){
                $("#appendRobotWidget form").submit();
            }
        }
    });

    $("#append").click(function(){
        $("#appendRobotWidget").dialog("open");
    });
});
